# пустой шаблон команды, загружающийся всегда перед реальными командами,
# чтобы реальная команда, если функции ниже не заданы в команде, отработала правильно

cmd_description="Описание не задано"
cmd_parse_mode="" # HTML, MarkdowsV2, Markdown

cmd_help() {
    local _cmd="$1"
    echo "$_cmd - помощь для команды не задана"
}

cmd_run() {
    echo "Команда не реализована"
}